<?xml version="1.0" encoding="utf-8"?>
<!-- dotted_line.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">

    <size
        android:width="2dp"
        android:height="1dp" />

    <stroke
        android:color="@color/grayscale10"
        android:dashWidth="5dp"
        android:dashGap="2dp" />
</shape>
