<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EditTextUnderline">
        <item name="colorControlNormal">@color/grayscale9</item>
        <item name="colorControlActivated">@color/primary3</item>
    </style>
    <style name="BottomNavigationTextAppearance" parent="TextAppearance.AppCompat.Caption">
        <item name="android:textSize">10dp</item>
        <item name="android:fontFamily">@font/pretendard_regular</item>
    </style>
    <declare-styleable name="CustomArcProgressBar">
        <attr name="cornerRadius" format="dimension"/>
        <attr name="radius" format="dimension"/>
        <attr name="angles" format="reference"/>
    </declare-styleable>
</resources>